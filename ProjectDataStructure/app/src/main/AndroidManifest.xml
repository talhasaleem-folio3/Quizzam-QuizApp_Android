<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.talha.projectdatastructure" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartingActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="Login Activity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".QuizInstruction"
            android:label="Instruction Panel"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".QuestionnaireActivity"
            android:label="Quiz"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ResultActivity"
            android:label="Final Result"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".AdminMenuActivity"
            android:label="Admin Menu"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".InsertQuestionsActivity"
            android:label="Insert Questions"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".DeleteQuestionsActivity"
            android:label="Delete Questions"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>
